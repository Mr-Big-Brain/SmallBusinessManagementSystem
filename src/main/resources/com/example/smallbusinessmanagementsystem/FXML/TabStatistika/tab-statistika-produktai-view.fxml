        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
        <ChoiceBox fx:id="choiceBoxTipas" layoutX="448.0" layoutY="23.0" prefWidth="150.0" stylesheets="@../../CSS/ChoiceBox.css" />
      <Button fx:id="buttonRodyti" layoutX="493.0" layoutY="577.0" mnemonicParsing="false" onAction="#rodyti" stylesheets="@../../CSS/ButtonOptional.css" text="Rodyti" />
      <Label layoutX="54.0" layoutY="6.0" stylesheets="@../../CSS/Label.css" text="Nuo" />
      <Label layoutX="246.0" layoutY="6.0" stylesheets="@../../CSS/Label.css" text="Iki" />
      <Label layoutX="448.0" layoutY="6.0" stylesheets="@../../CSS/Label.css" text="Tipas" />
    </children>
</AnchorPane>
