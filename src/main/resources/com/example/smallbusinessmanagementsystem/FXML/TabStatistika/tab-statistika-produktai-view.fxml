<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"

            prefHeight="400.0" prefWidth="600.0">
    <children>
        <LineChart fx:id="lineChartProduktai" layoutX="14.0" layoutY="59.0">
            <xAxis>
                <CategoryAxis side="BOTTOM" />
            </xAxis>
            <yAxis>
                <NumberAxis side="LEFT" />
            </yAxis>
        </LineChart>
        <TableView fx:id="tableViewProduktai" layoutX="630.0" layoutY="59.0" prefHeight="400.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="columnProduktas" prefWidth="75.0" text="Produktas" />
            </columns>
        </TableView>
        <Button fx:id="buttonPridetiProdukta" layoutX="631.0" layoutY="480.0" mnemonicParsing="false" onAction="#pridetiProdukta" text="Pridėti" />
        <Button fx:id="buttonPašalintiProdukta" layoutX="871.0" layoutY="480.0" mnemonicParsing="false" onAction="#pasalintiProdukta" text="Pašalinti" />
        <DatePicker fx:id="datePickerNuo" layoutX="50.0" layoutY="20.0" />
        <DatePicker fx:id="datePickerIki" layoutX="242.0" layoutY="20.0" />
        <Label layoutX="224.0" layoutY="451.0" text="Line chart" />
    </children>
</AnchorPane>
