<AnchorPane prefHeight="618.0" prefWidth="1306.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TableView fx:id="tableViewProduktai" layoutX="991.0" layoutY="75.0" prefHeight="227.0" prefWidth="284.0" stylesheets="@../../CSS/TableView.css">
            <columns>
                <TableColumn fx:id="columnProduktas" prefWidth="289.0" text="Produktas" />
            </columns>
        </TableView>
        <Button fx:id="buttonPridetiProdukta" layoutX="991.0" layoutY="323.0" mnemonicParsing="false" onAction="#pridetiProdukta" stylesheets="@../../CSS/ButtonCreate.css" text="Pridėti" />
        <Button fx:id="buttonPasalintiProdukta" layoutX="1213.0" layoutY="323.0" mnemonicParsing="false" onAction="#pasalintiProdukta" stylesheets="@../../CSS/ButtonDelete.css" text="Pašalinti" />
