<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TableView fx:id="tableViewProduktai" layoutX="820.0" layoutY="59.0" prefHeight="227.0" prefWidth="284.0" stylesheets="@../../CSS/TableView.css">
            <columns>
                <TableColumn fx:id="columnProduktas" prefWidth="289.0" text="Produktas" />
            </columns>
        </TableView>
        <Button fx:id="buttonPridetiProdukta" layoutX="820.0" layoutY="307.0" mnemonicParsing="false" onAction="#pridetiProdukta" stylesheets="@../../CSS/ButtonCreate.css" text="Pridėti" />
        <Button fx:id="buttonPasalintiProdukta" layoutX="1042.0" layoutY="307.0" mnemonicParsing="false" onAction="#pasalintiProdukta" stylesheets="@../../CSS/ButtonDelete.css" text="Pašalinti" />
        <DatePicker fx:id="datePickerNuo" layoutX="50.0" layoutY="20.0" />
        <DatePicker fx:id="datePickerIki" layoutX="242.0" layoutY="20.0" />
      <LineChart fx:id="lineChartProduktai" layoutX="44.0" layoutY="59.0" prefHeight="331.0" prefWidth="703.0" stylesheets="@../../CSS/LineChart.css">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
      <Button fx:id="buttonRodyti" layoutX="318.0" layoutY="413.0" mnemonicParsing="false" onAction="#rodyti" stylesheets="@../../CSS/ButtonOptional.css" text="Rodyti" />
    </children>
</AnchorPane>
