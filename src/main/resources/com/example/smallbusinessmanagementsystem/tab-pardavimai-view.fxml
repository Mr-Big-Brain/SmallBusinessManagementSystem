        <DatePicker fx:id="datePickerNuo" layoutX="14.0" layoutY="455.0" />
        <DatePicker fx:id="datePickerIki" layoutX="227.0" layoutY="455.0" />
        <Label layoutX="14.0" layoutY="438.0" text="Nuo" />
        <Label layoutX="227.0" layoutY="438.0" text="Iki" />
    </children>
</AnchorPane>
